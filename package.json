{
  "name": "px-stream",
  "version": "0.0.0",
  "devDependencies": {
    "eslint": "^9.34.0",
    "eslint-plugin-maxpat": "github:darosh/eslint-plugin-maxpat",
    "glob": "^11.0.3",
    "json-stringify-pretty-compact": "^4.0.0",
    "sharp": "^0.34.3"
  },
  "private": true,
  "scripts": {
    "amxd:freeze": "rm -f ./devices/frozen/*.amxd && osascript ./scripts/freeze.scpt \"$(pwd)/devices\" \"$(pwd)/devices/frozen\"",
    "amxd:resave": "osascript ./scripts/resave.scpt \"$(pwd)/devices\"",
    "doc": "node ./scripts/doc.js",
    "format:shader": "node ./scripts/format-shader.js",
    "lint:maxpat": "eslint ./devices/*.maxpat --rule={\"maxpat/ui/annotation-info\":\"off\"}",
    "lint:maxpat:fix": "eslint ./devices/*.maxpat --fix --rule={\"maxpat/ui/annotation-info\":\"off\"}",
    "lint:maxpat:fix-dry-run": "eslint ./devices/*.maxpat --fix-dry-run --rule={\"maxpat/ui/annotation-info\":\"off\"}",
    "lint:maxpat:fix:layout": "eslint ./devices/*.maxpat --fix --fix-type layout",
    "lint:maxpat:info": "eslint ./devices/*.maxpat --format node_modules/eslint-plugin-maxpat/lib/formatter.js",
    "measure": "node ./scripts/meausures.js",
    "screenshots": "rm -f ./assets/devices/*.webp && node ./scripts/screenshots.js",
    "screenshots:animated": "node ./scripts/screenshots.animated.js",
    "params:collect": "node scripts/params.js collect './devices/*.maxpat' './assets/params/'",
    "params:update": "node scripts/params.js update './devices/*.maxpat' './assets/params/*.maxpat.json'"
  },
  "type": "module"
}
